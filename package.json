{
    "name": "myRallyTool",
    "private": true,
    "version": "0.0.0",
    "description": "A tutorial application for AngularJS",
    "devDependencies": {
        "bower": "^1.7.7",
        "http-server": "^0.11.1",
        "jasmine-core": "^2.4.1",
        "karma": "^3.1.1",
        "karma-chrome-launcher": "^0.2.3",
        "karma-firefox-launcher": "^0.1.7",
        "karma-jasmine": "^0.3.8",
        "protractor": "^5.4.1"
    },
    "scripts": {
